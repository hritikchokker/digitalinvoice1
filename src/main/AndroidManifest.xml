<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hritik.hchok.mypersonaldiary">

    <permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/digital_diary"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat">
        <activity android:name=".activities.Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.Activities.secondActivity" />
        <activity android:name=".activities.Activities.NewEntryActivity" />
        <activity
            android:name=".activities.Activities.ListActivity"
            android:label="@string/title_activity_list"
            android:theme="@style/Theme.AppCompat"></activity>
        <activity android:name=".activities.Activities.DetailsActivity"></activity>
    </application>

</manifest>